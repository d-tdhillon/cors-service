{
  "scriptFile": "../../../dist/src/functions/adoProxy.js",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "request",
      "methods": ["get", "post", "put", "patch", "delete"],
      "route": "ado/{*path}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
